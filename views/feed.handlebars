<div class="container">
    <div class="section">
        <div class="row">
            <div id="feed" class="col s12 m6">
                <!-- TODO: Here is where all the user's lattest posts will go -->
                <!-- Might have to call in literals with this, especially for data? -->
                {{#each users}}
                    <div class="card">
                        <div class="card-content">
                            <h2>{{user.blogPost.title}}</h2>
                            <p>{{user.blogPost.body}}</p>
                            <p>by {{user}}</p>
                        </div>
                    </div>
                {{/each}}

            </div>
        </div>
    </div>
</div>

