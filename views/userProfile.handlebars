    </div>
</div>
<div class="card-content">
    <h3>{{this.result.dataValues.firstName}} {{this.result.dataValues.lastName}}'s Profile!</h3>
</div>
{{#each blogs}}
<div class="container">
    <div class="row">
        <div class="card blue-grey darken-1 col m10">
            <div class="card-content white-text">
                <span class="card-title">{{this.dataValues.title}}</span>
                <br>
                <p>{{this.dataValues.body}}</p>
                <br>
                <p>Created: {{this.dataValues.createdAt}}</p>
            </div>
        </div>
    </div>
</div>
{{/each}}









